{
  "name": "@graphile/pro",
  "version": "0.9.0",
  "description": "Professional PostGraphile enhancements",
  "main": "dist/index.js",
  "scripts": {
    "prerelease": "npm run test && rm -Rf dist && webpack --mode=production",
    "release": "echo 'Releasing...'",
    "postrelease": "npm publish --tag=next",
    "watch": "webpack --mode=development --watch",
    "build": "webpack --mode=production",
    "pretest": "psql -Xv ON_ERROR_STOP=1 pggql_test -f __tests__/kitchen-sink-schema.sql -f __tests__/kitchen-sink-data.sql",
    "test": "jest -i",
    "lint": "prettier --list-different 'src/**/*.ts' '__tests__/**/*.ts' && eslint 'src/**/*.ts'"
  },
  "keywords": [
    "postgraphile",
    "plugin",
    "graphile-build",
    "graphql",
    "pro",
    "production",
    "postgresql",
    "pg",
    "postgres"
  ],
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "@graphile/pg-pubsub": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "@typescript-eslint/typescript-estree": "^1.13.0",
    "eslint": "^6.1.0",
    "inquirer": "^6.3.1",
    "jest": "24.8.0",
    "mock-req": "^0.2.0",
    "mock-res": "^0.5.0",
    "postgraphile": "^4.4.0",
    "prettier": "^1.18.2",
    "semver": "^6.1.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3"
  },
  "dependencies": {
    "@types/graphql": "^14.2.0",
    "@types/pg": "^7.4.14",
    "graphql": ">=0.9 <15",
    "graphql-depth-limit": "^1.1.0",
    "pg": "^7.11.0"
  },
  "files": [
    "dist",
    "LICENSE.md"
  ]
}
